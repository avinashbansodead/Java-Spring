<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            
            <bean id="myPerson" class="personpandemo.Person">
            <property name="id" value="1"></property>
            <property name="name" value="Vishal"></property>
            <property name="address" value="Karvenagar"> </property>
            <property name="pan" ref="myPan"></property>  
            </bean>
            <bean id="myPan" class="personpandemo.Pan">
            <property name="id" value="1"></property>
            <property name="pannum" value="ABCD1234"></property>
            <property name="address" value="Karvenagar"></property>
            </bean>
            
            
    </beans>