<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            
    
          <bean id="mypen" class="springdemo1.Pen"></bean>
          <bean id="myPencil" class="springdemo1.Pencil"></bean>
          
          <bean id="myPen1" class="springdemo1.Pen1">
           <property name="id" value="1"></property>
           <property name="name" value="Cello"></property>
          <property name="price" value="10.00"></property>
          </bean>

<bean id="myPencil1" class="springdemo1.Pencil1">
           <property name="id" value="1"></property>
           <property name="brand" value="Nataraj"></property>
          <property name="price" value="7.00"></property>
          </bean>
         
    <bean id="myCompany" class="springdemo1.Company">
    <property name="id" value="1"></property>
    <property name="name" value="Wipro"></property>
    <property name="noofemp" value="20000"></property>
    </bean>

</beans>
